{
  "manifest_version": 2,

  "name": "YoutubeController",
  "description": "Control you Youtube song anywhere at home",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation"
  ]
}
